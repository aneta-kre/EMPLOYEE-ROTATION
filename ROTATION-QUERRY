SELECT
  [EMPLOYES_OCTOBER].[NAME] AS NAME_OCT
  [EMPLOYES_OCTOBER].[SECURITY_NUMBER] AS NUMBER_OCT
  [EMPLOYES_OCTOBER].[ID] AS ID_OCT
  [EMPLOYES_OCTOBER].[DEPARTEMENT] AS DEP_OCT

  [EMPLOYES_NOVEMBER].[NAME] AS NAME_NOV
  [EMPLOYES_NOVEMBER].[SECURITY_NUMBER] AS NUMBER_NOV
  [EMPLOYES_NOVEMBER].[ID] AS ID_NOV
  [EMPLOYES_NOVEMBER].[DEPARTEMENT] AS DEP_NOV

FROM
  [EMPLOYES_NOVEMBER]

LEFT OUTER JOIN ON [EMPLOYES_OCTOBER]
